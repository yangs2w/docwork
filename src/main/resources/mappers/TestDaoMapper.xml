<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mdds.mapper.TestDao">
    <select id="selectDeptByName" parameterType="String" resultType="com.mdds.entity.HpDepartment">
        select * from mc_dict_dept where dept_name = #{deptName};
    </select>
</mapper>